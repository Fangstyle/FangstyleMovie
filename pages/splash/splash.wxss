.container{
  display: flex;
  flex-direction:column;
  align-items: center;
  background-color:#fff;
}

.avatar{
  width:200rpx;
  height:200rpx;
  margin-top:160rpx;
}

.motto{
  margin-top:100rpx;
  font-size:32rpx;
  font-weight: bold;
}

.journey-container{
  margin-top: 200rpx;
  border: 1px solid #405f80;
  width: 200rpx;
  height: 80rpx;
  border-radius: 5px;
  text-align:center;
}

.journey{
  font-size:22rpx;
  font-weight: bold;
  line-height:80rpx;
  color: #405f80;
}

page{
  height: 100%;
  background-color:#b3d4db;
}

